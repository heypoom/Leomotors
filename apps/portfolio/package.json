{
  "name": "@leomotors/portfolio",
  "version": "3.0.137",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "yarn lm-config && vite dev --port=5274",
    "build": "yarn lm-config && vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint src *.{cjs,js} --max-warnings=0",
    "format": "prettier --write \"*.config.{cjs,js,ts}\" \"src/**/*.{ts,svelte,css,html,scss}\"",
    "deps": "ncu -u"
  },
  "devDependencies": {
    "@leomotors/config": "*",
    "@leomotors/scripts": "^3.1.0",
    "@leomotors/sv-components": "^0.4.0",
    "@sveltejs/adapter-vercel": "1.0.0-next.65",
    "@sveltejs/kit": "1.0.0-next.394",
    "@types/tailwindcss": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "^10.4.7",
    "chalk": "^5.0.1",
    "cssnano": "^5.1.12",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sass": "^1.54.0",
    "svelte": "^3.49.0",
    "svelte-bootstrap-icons": "^2.2.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.6",
    "tailwindcss-global-dark": "^1.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.3"
  },
  "dependencies": {
    "@leomotors/data": "*"
  }
}
