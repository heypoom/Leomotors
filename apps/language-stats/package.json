{
  "name": "@leomotors/language-stats",
  "version": "2.0.2",
  "description": "Get Summary of Your Language Stats",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "load": "mkdir -p data && node dist/src/loadData.js",
    "analyze": "node dist/src/analyze.js",
    "gql-codegen": "graphql-codegen --config codegen.yml",
    "lint": "eslint src --max-warnings=0",
    "format": "prettier --write .*.cjs src/**/*.{js,ts}",
    "deps": "ncu -u"
  },
  "dependencies": {
    "node-fetch": "^3.2.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.9.1",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-operations": "2.5.2",
    "@leomotors/config": "*",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "graphql": "^16.5.0",
    "prettier": "latest",
    "typescript": "^4.7.4"
  }
}
